Proceso ejemplo05
	// Primero definimos variables
	Definir num1, num2, resultado como Real;
	// Luego pedimos los numeros al usuario
	Escribir "Digite el valor del numero";
	Leer num1;
	Escribir "Digite el valor del segundo numero ";
	Leer num2;
	// Hacemos la operacion
	resultado <- num1 + num2;
	// Imprimimos el resultado
	Escribir "El resultado de sumar ", num1 , "+" , num2 , " es: " , resultado;
FinProceso

