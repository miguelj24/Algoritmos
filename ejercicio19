Proceso ejercicio19
	// Delcaracion de variables
	Definir num1, num2, resto como Entero;
	// Escribir los valores por consola
	Escribir "Ingresa los dos numeros";
	Leer num1, num2;
	// PROCESO - SALIDA
	resto <- num1 % num2;
	Si (resto = 0 )Entonces
		Escribir num1, " Es multiplo de: ", num2;
	SiNo
		Escribir num1, " No es multiplo de: ", num2;
	FinSi
FinProceso
