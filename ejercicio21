Proceso ejercicio21
	// Definimos Variables
	Definir num1, num2 como Real;
	// Pedir los numeros por consola
	Escribir "Ingresa los dos numeros";
	Leer num1, num2;
	// ENTRADA - SALIDA
	Si (num1 > num2) o (num2 < num1)Entonces
		Escribir "El ", num1, " Es mayor que ", num2;
	SiNo
		Si (num1 = num2)Entonces
			Escribir "El ", num1, " Es igual que ", num2;
		SiNo
			Escribir "El ", num1, " Es menor que el ", num2;
		FinSi
	FinSi
FinProceso
