Proceso ejercicio23
	// Definimos las variables
	Definir num1, num2, num3 como Real;
	// Pedimos numeros por consola
	Escribir "Ingresa los tres numeros que desea organizar";
	Leer num1, num2, num3;
	// ENTRADA - SALIDA
	Si (num1>num2 y num2>num3 )Entonces
		Escribir "El orden de los numeros es: ", num1, num2, num3;
	SiNo
		Si (num2>num1 y num1>num3 )Entonces
			Escribir "El orden de los numeros es: ", num2, num1, num3;
		SiNo
			Si (num3>num2 y num2>num1 )Entonces
				Escribir "El orden de los numeros es: ", num3, num2, num1;
			SiNo
				Si (num1>num3 y num3>num2 )Entonces
					Escribir "El orden de los numeros es: ", num1, num3, num2;
				SiNo
					Si (num2>num3 y num3>num1)Entonces
						Escribir "El orden de los numeros es: ", num2, num3, num1;
					SiNo
						Si (num3>num1 y num1>num2)Entonces
							Escribir "El orden de los numeros es: ", num3, num1, num2;
						FinSi
					FinSi
				FinSi
			FinSi
		FinSi
	FinSi
FinProceso
